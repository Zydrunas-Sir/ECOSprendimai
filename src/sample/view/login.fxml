<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.LoginController">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="300.0" prefWidth="500.0">
         <children>
            <Label alignment="TOP_CENTER" layoutX="100.0" layoutY="37.0" prefHeight="47.0" prefWidth="293.0" text="EcoSprendimai">
               <font>
                  <Font name="Comic Sans MS" size="27.0" />
               </font>
            </Label>
            <Label alignment="CENTER_RIGHT" layoutX="44.0" layoutY="100.0" prefHeight="37.0" prefWidth="172.0" text="Elektroninis paštas:">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font></Label>
            <Label alignment="CENTER_RIGHT" layoutX="44.0" layoutY="144.0" prefHeight="37.0" prefWidth="172.0" text="Slaptažodis:" textAlignment="RIGHT">
               <font>
                  <Font name="Comic Sans MS" size="17.0" />
               </font></Label>
            <TextField fx:id="email_textfield" layoutX="222.0" layoutY="100.0" prefHeight="37.0" prefWidth="220.0">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></TextField>
            <PasswordField fx:id="password_passwordfield" layoutX="222.0" layoutY="144.0" prefHeight="37.0" prefWidth="220.0">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="TEXT" />
               </cursor></PasswordField>
            <Label fx:id="login_info_label" alignment="CENTER" layoutX="7.0" layoutY="187.0" prefHeight="47.0" prefWidth="475.0" textFill="RED" wrapText="true">
               <font>
                  <Font name="Serif Regular" size="20.0" />
               </font></Label>
            <Button fx:id="username_button" layoutX="121.0" layoutY="237.0" mnemonicParsing="false" onAction="#login" prefHeight="37.0" prefWidth="108.0" text="Prisijungti">
               <font>
                  <Font name="Comic Sans MS" size="16.0" />
               </font></Button>
            <Button fx:id="register_button" layoutX="332.0" layoutY="237.0" mnemonicParsing="false" onAction="#register" prefHeight="37.0" prefWidth="108.0" text="Registruoti">
               <font>
                  <Font name="Comic Sans MS" size="16.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Label layoutX="440.0" layoutY="12.0" prefHeight="25.0" prefWidth="43.0" text="v: 0.1" textFill="#b9b9b9" />
            <Button fx:id="dashboard_button" layoutX="9.0" layoutY="237.0" mnemonicParsing="false" onAction="#goTodashboard" prefHeight="37.0" prefWidth="108.0" text="Dashboard">
               <font>
                  <Font name="Comic Sans MS" size="16.0" />
               </font></Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets></Pane>
   </children>
</GridPane>
